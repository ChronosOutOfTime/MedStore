{
    "extends": "react-app",
    "rules": {
        "comma-dangle": ["error", "always-multiline"],
        "no-mixed-spaces-and-tabs": "error",
        "indent": ["error", "tab"],
        "semi": ["error", "always"],
        "keyword-spacing": [2, {"after": true}], 
        "space-before-blocks": "error",
        "space-infix-ops": "error",
        "space-unary-ops": "error"
    }
}